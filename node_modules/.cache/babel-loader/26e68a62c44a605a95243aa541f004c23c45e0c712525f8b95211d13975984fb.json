{"ast":null,"code":"import { required, email, minLength } from '@vuelidate/validators';\nimport { reactive, computed } from 'vue';\nimport { useVuelidate } from '@vuelidate/core';\nexport default {\n  data() {\n    return {\n      isOpen: false,\n      formData: reactive({\n        meno: '',\n        email: ''\n      }),\n      v$: null\n    };\n  },\n  methods: {\n    openModal() {\n      this.isOpen = true;\n      console.log(\"Modální okno otvorené\");\n      this.$nextTick(() => {\n        const modal = document.getElementById('modal');\n        if (modal) {\n          console.log(\"Modální okno nájdené, posúvanie...\");\n          modal.scrollIntoView({\n            behavior: 'smooth'\n          });\n        } else {\n          console.log(\"Modální okno nenájdené\");\n        }\n      });\n    },\n    async odoslatFormular() {\n      if (this.v$) {\n        const result = await this.v$.$validate();\n        if (result) {\n          alert('Formulár bol úspešne odoslaný!');\n        } else {\n          alert('Prosím, vyplňte povinné polia.');\n        }\n      }\n    }\n  },\n  watch: {\n    isOpen(value) {\n      if (value) {\n        document.getElementById('app').classList.add('modal-open');\n      } else {\n        document.getElementById('app').classList.remove('modal-open');\n      }\n    }\n  },\n  computed: {\n    rules() {\n      return computed(() => ({\n        meno: {\n          required,\n          minLength: minLength(5)\n        },\n        email: {\n          required,\n          email\n        }\n      }));\n    }\n  },\n  validations() {\n    return {\n      formData: this.rules\n    };\n  },\n  mounted() {\n    this.v$ = useVuelidate(this.rules, this.formData);\n  }\n};","map":{"version":3,"names":["required","email","minLength","reactive","computed","useVuelidate","data","isOpen","formData","meno","v$","methods","openModal","console","log","$nextTick","modal","document","getElementById","scrollIntoView","behavior","odoslatFormular","result","$validate","alert","watch","value","classList","add","remove","rules","validations","mounted"],"sources":["/Users/jozefkusnierik/Documents/GitHub/Frontend-Developer/src/components/HeaderComponent.vue"],"sourcesContent":["<template>\n   <header>\n            <div class=\"logo-btn\"> \n                <img src=\"./../assets/images/logoR.png\" alt=\"logo\" class=\"logo\">\n                <button @click=\"openModal\">Mám záujem o UX audit</button>\n                <teleport to=\"#app\">\n                    <div id=\"modal\"  class=\"bg-modal\" v-if=\"isOpen\">\n                        <div class=\"modal-header\">\n                            <h1>Jednoducho nám napíšte a my sa vám ozveme.</h1>\n                            <img src=\"./../assets/images/exit.png\" alt=\"\" @click=\"isOpen = false\">\n                        </div>\n                        <form @submit.prevent=\"odoslatFormular\">\n                            <div class=\"name\">\n                                <p>Meno <span>*</span></p>\n                                <input type=\"text\" name=\"meno\" id=\"meno\" v-model=\"formData.meno\">\n                                <span v-for=\"error in v$.meno.$errors\" :key=\"error.$uid\" class=\"error\"> \n                                    {{ error.$message }}\n                                </span>\n                            </div>\n                            <div class=\"mail-tel\">\n                                <div class=\"modal-mail\">\n                                    <p>E-mail: <span>*</span> </p>\n                                    <input type=\"email\" name=\"email\" id=\"email\"  v-model=\"formData.email\">\n                                     <span v-for=\"error in v$.email.$errors\" :key=\"error.$uid\" class=\"error\"> \n                                    {{ error.$message }}\n                                </span>\n                                </div>\n                                <div class=\"modal-tel\">\n                                    <p>Tel.číslo:</p>\n                                    <input type=\"text\" name=\"\" id=\"\">\n                                </div>\n                            </div>\n                            <div class=\"web\">\n                                <p>Webstránka:</p>\n                                <input type=\"text\" name=\"\" id=\"\">\n                            </div>\n                            <div class=\"note\">\n                                <p>Poznámka:</p>\n                                <input type=\"text\" name=\"\" id=\"\" placeholder=\"Je niečo čo by ste sa nás chceli spýtat?\">\n                            </div>\n                             <div class=\"modal-btn\">\n                                <button type=\"submit\">Kontaktujte ma</button>\n                            </div>\n                        </form>\n                    </div>\n                </teleport>\n            </div>\n            <div class=\"title\">\n                <h1>Zvýšte svoje <span>zisky optimalizáciou</span>vášho eshopu.</h1>    \n            </div>\n            <div class=\"mobile-btn\">\n                 <button @click=\"openModal\">Mám záujem o UX audit</button>\n            </div>\n        </header>\n</template>\n\n<script>\nimport { required, email, minLength} from '@vuelidate/validators';\nimport { reactive, computed } from 'vue';\nimport { useVuelidate } from '@vuelidate/core';\n\nexport default {\n  data() {\n    return {\n      isOpen: false,\n      formData: reactive({\n        meno: '',\n        email: ''\n      }),\n      v$: null\n    };\n  },\n  methods: {\n    openModal() {\n      this.isOpen = true;\n      console.log(\"Modální okno otvorené\");\n      this.$nextTick(() => {\n        const modal = document.getElementById('modal');\n        if (modal) {\n          console.log(\"Modální okno nájdené, posúvanie...\");\n          modal.scrollIntoView({ behavior: 'smooth'});\n        } else {\n          console.log(\"Modální okno nenájdené\");\n        }\n      });\n    },\n    async odoslatFormular() {\n      if (this.v$) {\n        const result = await this.v$.$validate();\n        if (result) {\n          alert('Formulár bol úspešne odoslaný!');\n        } else { \n          alert('Prosím, vyplňte povinné polia.');\n        }\n      }\n    }\n  },\n  watch: {\n    isOpen(value) {\n      if (value) {\n        document.getElementById('app').classList.add('modal-open');\n      } else {\n        document.getElementById('app').classList.remove('modal-open');\n      }\n    }\n  },\n  computed: {\n    rules() {\n      return computed(() => ({\n        meno: { required, minLength: minLength(5) },\n        email: { required, email }\n      }));\n    }\n  },\n  validations() {\n    return {\n      formData: this.rules\n    };\n  },\n  mounted() {\n    this.v$ = useVuelidate(this.rules, this.formData);\n  }\n};\n</script>\n\n<style>\nheader {\n    width: 100vw;\n    height: 34.625em;\n    background: linear-gradient(171deg, #3D2E80 51.38%, #DC1969 125.72%);\n}\n.logo-btn {\n    display: flex;\n    justify-content: space-around;\n    padding-top: 3.56em;\n}\n.logo {\n    width: 9em;\n    height: 3em;\n}\n.logo-btn button{\n    width: 13.5em;\n    height: 3em;\n    border-radius: 0.5em;\n    background: #EE325C;\n    border: none;\n    color: #FFF;\n    text-align: center;\n    font-size: 0.875em;\n    font-weight: 900;\n}\n.title {\n    display: flex;\n    height: 20.3125em;\n    justify-content: center;\n    padding-top: 0.31em;\n}\nh1 {\n    color: #FFF;\n    text-align: center;\n    font-size: 3.125em;\n    font-style: normal;\n    font-weight: 900;\n    line-height: 2em;\n    width: 14.375em;\n}\n.mobile-btn {\n    display: none;\n}\n\n/* Modalný formulár */\n#app.modal-open {\n  opacity: 0.5;\n  background-color: rgba(255 , 255, 255, 0.90);\n}\n\n.bg-modal {\n    position: absolute;\n    width: 34.19em;\n    border: 1px solid #E7E8EB;\n    background: #fff;\n    box-shadow: 0px 32px 45px 0px rgba(0, 0, 0, 0.15);\n    z-index: 2;\n    display: flex;\n    flex-direction: column;\n    top: 7em;\n}\n\n.modal-header {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n}\n\n.bg-modal h1{\n    font-size: 1.875em;\n    color: #000;\n    width: 12.75em;\n    height: 6.625em;\n    font-weight: 900;\n    line-height: 2em;\n    margin-top: 3em;\n}\n.bg-modal img {\n    position: relative;\n    top: -17em;\n    left: 14em;\n    cursor: pointer;\n}\n\n.bg-modal form {\n    margin-top: -7em;\n    margin-left: 3em;\n}\n\n.bg-modal input {\n    width: 32em;\n    height: 3.44em;\n    border-radius: 0.25em;\n    background-color: #EFEFEF;\n    border: #EFEFEF;\n    padding-left: 2em;\n}\n.bg-modal p {\n    font-weight: 900;\n}\n.bg-modal span {\n    color: #EE325C;\n}\n.name {\n    display: flex;\n    flex-direction: column;\n}\n.mail-tel {\n    display: flex;\n}\n.mail-tel input {\n    width: 13em;\n}\n\n.modal-mail {\n    margin-right: 3em;\n    display: flex;\n    flex-direction: column;\n}\n\n.note input{\n    padding-bottom: 10em;\n}\n.note input::placeholder {\n    color: #000;\n    font-weight: 400;\n    font-size: 1em;\n}\n.modal-btn {\n    display: flex;\n    margin-bottom: 1em;\n}\nButton {\n    width: 23em;\n    height: 3.5em;\n    border-radius: 0.5em;\n    background: #EE325C;\n    border: none;\n    color: #FFF;\n    text-align: center;\n    font-size: 1.25em;\n    font-weight: 900;\n    margin-top: 1em;\n    transition-duration: 2s;\n    cursor: pointer;\n}\n.error {\n    color: #EE325C;\n    font-weight: bold;\n}\n\n\n@media screen and (max-width: 560px ) {\n     .title{\n        height: 12.31em;\n        margin-top: 3em;\n    }\n    .title h1 {\n        font-size: 1.875em;\n        height: 3em;\n    }\n    .logo-btn button {\n        display: none;    \n    }\n    .bg-modal {\n        font-size: 0.6em;\n    }\n    .bg-modal input {\n        width: 19em;\n    }\n    .mail-tel input {\n        width: 7.3em;\n    }\n    .mobile-btn {\n        display: flex;\n        justify-content: center;    \n        margin-top: 4em;\n    }\n    .mobile-btn Button {\n        width: 15em;\n        font-size: 0.88em;\n        justify-content: center;\n    }\n    .modal-btn Button {\n        width: 23.5em;\n        justify-content: center;\n    }\n    .bg-modal form {\n        margin-left: 2.5em;\n    }\n}\n\n@media screen and (max-width: 360px) {\n    .logo-btn {\n        justify-content: start;\n        margin-left: 1em;\n    }\n    .logo-btn button {\n        right: 8em;\n        min-width: 15.5em;    \n    }\n    .title h1 {\n        display: flex; \n        flex-direction: column;   \n    }\n}\n</style>"],"mappings":"AAyDA,SAASA,QAAQ,EAAEC,KAAK,EAAEC,SAAS,QAAO,uBAAuB;AACjE,SAASC,QAAQ,EAAEC,QAAO,QAAS,KAAK;AACxC,SAASC,YAAW,QAAS,iBAAiB;AAE9C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,KAAK;MACbC,QAAQ,EAAEL,QAAQ,CAAC;QACjBM,IAAI,EAAE,EAAE;QACRR,KAAK,EAAE;MACT,CAAC,CAAC;MACFS,EAAE,EAAE;IACN,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACL,MAAK,GAAI,IAAI;MAClBM,OAAO,CAACC,GAAG,CAAC,uBAAuB,CAAC;MACpC,IAAI,CAACC,SAAS,CAAC,MAAM;QACnB,MAAMC,KAAI,GAAIC,QAAQ,CAACC,cAAc,CAAC,OAAO,CAAC;QAC9C,IAAIF,KAAK,EAAE;UACTH,OAAO,CAACC,GAAG,CAAC,oCAAoC,CAAC;UACjDE,KAAK,CAACG,cAAc,CAAC;YAAEC,QAAQ,EAAE;UAAQ,CAAC,CAAC;QAC7C,OAAO;UACLP,OAAO,CAACC,GAAG,CAAC,wBAAwB,CAAC;QACvC;MACF,CAAC,CAAC;IACJ,CAAC;IACD,MAAMO,eAAeA,CAAA,EAAG;MACtB,IAAI,IAAI,CAACX,EAAE,EAAE;QACX,MAAMY,MAAK,GAAI,MAAM,IAAI,CAACZ,EAAE,CAACa,SAAS,CAAC,CAAC;QACxC,IAAID,MAAM,EAAE;UACVE,KAAK,CAAC,gCAAgC,CAAC;QACzC,OAAO;UACLA,KAAK,CAAC,gCAAgC,CAAC;QACzC;MACF;IACF;EACF,CAAC;EACDC,KAAK,EAAE;IACLlB,MAAMA,CAACmB,KAAK,EAAE;MACZ,IAAIA,KAAK,EAAE;QACTT,QAAQ,CAACC,cAAc,CAAC,KAAK,CAAC,CAACS,SAAS,CAACC,GAAG,CAAC,YAAY,CAAC;MAC5D,OAAO;QACLX,QAAQ,CAACC,cAAc,CAAC,KAAK,CAAC,CAACS,SAAS,CAACE,MAAM,CAAC,YAAY,CAAC;MAC/D;IACF;EACF,CAAC;EACDzB,QAAQ,EAAE;IACR0B,KAAKA,CAAA,EAAG;MACN,OAAO1B,QAAQ,CAAC,OAAO;QACrBK,IAAI,EAAE;UAAET,QAAQ;UAAEE,SAAS,EAAEA,SAAS,CAAC,CAAC;QAAE,CA